{
  "name": "Patiententagebuch",
  "version": "1.0.0",
  "description": "Patiententagebuch is an app to keep track of your medications and body sign measurements. To have an overview you can see how those develop over time in different charts.",
  "main": "server.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:client dev:server",
    "dev:client": "cd client && npm start",
    "dev:server": "nodemon server.js",
    "test": "cd client && npm test",
    "storybook": "cd client && npm run storybook",
    "build": "cd client && npm run build",
    "postinstall": "cd client && npm install",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anniviz/capstone-project.git"
  },
  "author": "Annemarie Wisbar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anniviz/capstone-project/issues"
  },
  "homepage": "https://github.com/anniviz/capstone-project#readme",
  "dependencies": {
    "express": "^4.17.1",
    "mongodb": "^4.1.3",
    "mongoose": "^6.0.12",
    "nodemon": "^2.0.14",
    "npm-run-all": "^4.1.5"
  }
}
